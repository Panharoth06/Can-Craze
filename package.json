{
  "dependencies": {
    "@tailwindcss/cli": "^4.1.5",
    "concurrently": "^9.1.2",
    "flowbite": "^3.1.2",
    "live-server": "^1.2.0"
  },
  "scripts": {
    "dek": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "live-server": "live-server --port=3000",
    "concur": "concurrently \"npm run dek\" \"npm run live-server\""
  }
}
